{
    "name": "iksaku/siase-wrapper",
    "description": "A PHP Wrapper for various endpoints in the SIASE system",
    "type": "library",
    "license": "AGLP-3.0",
    "authors": [
        {
            "name": "Jorge GonzÃ¡lez",
            "email": "iksaku@me.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-simplexml": "*",
        "nesbot/carbon": "^2.22",
        "ext-json": "*",
        "symfony/serializer": "^4.3",
        "symfony/property-access": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "extra": {
        "hooks": {
            "pre-commit": [
                "php-cs-fixer fix"
            ],
            "pre-push": [
                "php-cs-fixer fix --dry-run"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "SIASE\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SIASE\\Tests\\": "tests/"
        }
    }
}
